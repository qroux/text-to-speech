{
  "expo": {
    "name": "text-to-speech",
    "slug": "text-to-speech",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon2.png",
    "scheme": "texttospeech",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.qoux.texttospeech"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.qoux.texttospeech"
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    }
  }
}
